<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeActivity">

    <ImageView
        android:id="@+id/home"
        android:layout_width="160dp"
        android:layout_height="131dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="224dp"
        android:layout_marginBottom="400dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pib" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="179dp"
        android:layout_height="30dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="205dp"
        android:layout_marginBottom="350dp"
        android:text="Please a category"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home"
        app:layout_constraintVertical_bias="0.76" />

    <ImageView
        android:id="@+id/gd"
        android:layout_width="109dp"
        android:layout_height="99dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="217dp"
        android:layout_marginBottom="176dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.465"
        app:srcCompat="@drawable/gd" />

    <ImageView
        android:id="@+id/an"
        android:layout_width="98dp"
        android:layout_height="100dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="133dp"
        android:layout_marginBottom="200dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/gd"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/am" />

    <ImageView
        android:id="@+id/app"
        android:layout_width="99dp"
        android:layout_height="101dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="424dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="207dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.433"
        app:layout_constraintStart_toEndOf="@+id/an"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.941"
        app:srcCompat="@drawable/app" />

    <ImageView
        android:id="@+id/va"
        android:layout_width="90dp"
        android:layout_height="93dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="160dp"
        android:layout_marginBottom="70dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gd"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/va" />

    <ImageView
        android:id="@+id/ws"
        android:layout_width="92dp"
        android:layout_height="90dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="85dp"
        app:layout_constraintBottom_toTopOf="@+id/va"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/va"
        app:layout_constraintTop_toBottomOf="@+id/app"
        app:layout_constraintVertical_bias="0.09"
        app:srcCompat="@drawable/ws" />

    <ImageView
        android:id="@+id/curt"
        android:layout_width="60dp"
        android:layout_height="69dp"
        android:layout_marginStart="148dp"
        android:layout_marginTop="114dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="241dp"
        app:layout_constraintBottom_toTopOf="@+id/app"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/home"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/curt" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="70dp"
        android:layout_height="36dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="196dp"
        android:text="Checkout"
        app:layout_constraintBottom_toTopOf="@+id/app"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/home"
        app:layout_constraintTop_toBottomOf="@+id/curt"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>