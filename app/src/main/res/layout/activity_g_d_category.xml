<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GDCategory">

    <ImageView
        android:id="@+id/home"
        android:layout_width="137dp"
        android:layout_height="119dp"
        android:layout_marginStart="47dp"
        android:layout_marginTop="59dp"
        android:layout_marginEnd="227dp"
        android:layout_marginBottom="553dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pib" />

    <TextView
        android:id="@+id/rghj"
        android:layout_width="132dp"
        android:layout_height="32dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="65dp"
        android:layout_marginEnd="227dp"
        android:layout_marginBottom="456dp"
        android:text="Logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="123dp"
        android:layout_height="30dp"
        android:layout_marginStart="33dp"
        android:layout_marginEnd="204dp"
        android:layout_marginBottom="349dp"
        android:text="Advertisement Flayer"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/adflr"
        android:layout_width="44dp"
        android:layout_height="45dp"
        android:layout_marginStart="87dp"
        android:layout_marginEnd="73dp"
        android:layout_marginBottom="53dp"
        app:layout_constraintBottom_toTopOf="@+id/magazine"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:srcCompat="@drawable/curt" />

    <ImageView
        android:id="@+id/magazine"
        android:layout_width="44dp"
        android:layout_height="45dp"
        android:layout_marginEnd="73dp"
        android:layout_marginBottom="236dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/curt" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="116dp"
        android:layout_height="32dp"
        android:layout_marginStart="41dp"
        android:layout_marginTop="73dp"
        android:layout_marginEnd="86dp"
        android:layout_marginBottom="244dp"
        android:text="Magazine"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/magazine"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="43dp"
        android:layout_height="55dp"
        android:layout_marginStart="85dp"
        android:layout_marginBottom="38dp"
        app:layout_constraintBottom_toTopOf="@+id/adflr"
        app:layout_constraintStart_toEndOf="@+id/rghj"
        app:srcCompat="@drawable/curt" />

    <Button
        android:id="@+id/Check out"
        android:layout_width="156dp"
        android:layout_height="46dp"
        android:layout_marginStart="102dp"
        android:layout_marginTop="77dp"
        android:layout_marginEnd="102dp"
        android:layout_marginBottom="113dp"
        android:text="Check Out"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/magazine" />
</androidx.constraintlayout.widget.ConstraintLayout>